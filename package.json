  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "npm run typecheck && npm run lint:check && npm run build",
    "proof": "tsx scripts/proof_phase9.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "tsx --require dotenv/config scripts/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "prepare": "husky install || true",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "prisma:status": "prisma migrate status --schema=prisma/schema.prisma"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
